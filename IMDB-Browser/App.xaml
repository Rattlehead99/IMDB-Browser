<Application x:Class="IMDB_Browser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:IMDB_Browser"
             xmlns:viewModels="clr-namespace:IMDB_Browser.ViewModels"
             xmlns:views="clr-namespace:IMDB_Browser.Views"
             Startup="App_OnStartup">
    <Application.Resources>
         <ResourceDictionary>
             
             <DataTemplate DataType="{x:Type viewModels:MovieListViewModel}">
                    <views:MovieListView/>
             </DataTemplate>

         </ResourceDictionary>
    </Application.Resources>
</Application>
